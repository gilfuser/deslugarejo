{
  "name": "deslugarejo",
  "version": "0.0.2",
  "description": "non-local jamming",
  "author": "Gil Fuser <gilfuser@gmail.com>",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "com.deslugarejo.app",
    "directories": {
      "buildResources": "static"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=DEV electron .",
    "build": "nuxt build && electron-builder",
    "package-mac": "nuxt build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win32": "nuxt build && electron-packager . deslugar --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Deslugar\"",
    "package-win64": "nuxt build && electron-packager . deslugar --overwrite --asar=true --platform=win64 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Deslugar\"",
    "package-linux": "nuxt build && electron-packager . deslugar --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/vuetify": "^0.5.5",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "firebase": "^5.9.2",
    "node-osc": "^3.0.0",
    "nuxt": "^2.4.0",
    "signalhub": "^4.9.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "vue-async-computed": "^3.6.1",
    "vue-moment": "^4.0.0",
    "vuelidate": "^0.7.4",
    "vuexfire": "^2.3.1",
    "webrtc-swarm": "QwantResearch/webrtc-swarm",
    "wrtc": "^0.3.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.1.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "strip-ansi": "^5.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-loader": "^15.7.0",
    "vuetify-loader": "^1.2.1"
  }
}
